{
  "name": "mobx-preact",
  "version": "3.1.0",
  "description": "Mobx bindings specifically for Preact",
  "main": "lib/index.js",
  "module": "lib/index.module.js",
  "types": "types.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/philmander/mobx-preact.git"
  },
  "scripts": {
    "lint": "eslint src test --fix",
    "prepublish": "npm run lint && npm test && npm run build",
    "build": "node build-rollup",
    "test": "jest",
    "build:sample": "mkdirp sample/dist && babel sample/sample.js -o sample/dist/sample.es5.js && rollup -c sample/rollup.config.js",
    "release": "standard-version"
  },
  "keywords": [
    "preact",
    "mobx-preact",
    "mobx",
    "observer",
    "bindings"
  ],
  "author": "Phil Mander",
  "license": "MIT",
  "peerDependencies": {
    "mobx": "5.x"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/plugin-transform-react-jsx": "^7.3.0",
    "@babel/preset-env": "^7.5.5",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.8.0",
    "eslint": "^6.1.0",
    "eslint-plugin-jest": "^22.13.6",
    "eslint-plugin-react": "^7.5.1",
    "jest": "^24.8.0",
    "mobx": "^5.0.0",
    "preact": "^10.0.0-rc.0",
    "rollup": "^1.17.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.0.1",
    "rollup-plugin-filesize": "^6.1.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-terser": "^5.1.1",
    "standard-version": "^6.0.1"
  },
  "dependencies": {
    "core-js": "^3.1.4",
    "hoist-non-react-statics": "^3.3.0",
    "regenerator-runtime": "^0.13.3"
  }
}
